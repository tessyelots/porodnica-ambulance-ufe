{
    "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "generator-cli": {
        "useDocker": true,
        "version": "6.6.0",
        "generators": {
            "porodnica-ambulance-home": {
                "generatorName": "typescript-fetch"   ,
                "glob": "api/porodnica-ambulance-home.openapi.yaml",
                "output": "#{cwd}/src/api/porodnica-ambulance-home",
                "additionalProperties": {
                    "supportsES6": "true",
                    "withInterfaces": true
                }
            }
        }
    }
  }